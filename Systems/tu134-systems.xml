<?xml version="1.0"?>

<!--
07.2015 - file modified by lastmin-II: boosted vacuum system because of
low engine rpm; this was needed to allow heading indicator functionality.
-->

<PropertyList>

  <electrical>
    <name>electrical</name>
    <number>0</number>
    <path>Aircraft/Generic/generic-electrical.xml</path>
  </electrical>

  <pitot>
    <name>pitot</name>
    <number>0</number>
  </pitot>

  <static>
    <name>static</name>
    <number>0</number>
    <tau>1</tau>
  </static>

  <vacuum>
    <name>vacuum</name>
    <number>0</number>
    <rpm>/engines/engine[0]/rpm</rpm>
    <scale>20.0</scale>
  </vacuum>

</PropertyList>
