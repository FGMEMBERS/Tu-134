  <!-- Porte -->
  <animation>
    <name>LaPorte</name>
    <object-name>porte</object-name>
    <object-name>vitreporte</object-name>
  </animation>

  <animation>
    <type>pick</type>
    <object-name>LaPorte</object-name>
    <visible>true</visible>
    <action>
      <button>0</button>
      <repeatable>false</repeatable>
      <binding>
        <command>nasal</command>
        <script>doors.crew.toggle();</script>
      </binding>
    </action>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>LaPorte</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep>  0 </dep></entry>
      <entry><ind> 0.2 </ind><dep>  0 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 65 </dep></entry>
    </interpolation>
    <center>
      <x-m> -13.055 </x-m>
      <y-m>   0.000 </y-m>
      <z-m>  -1.188 </z-m>
    </center>
    <axis>
      <x> -1 </x>
      <y>  0 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>translate</type>
    <object-name>LaPorte</object-name>
    <property>sim/model/door-positions/crew/position-norm</property>
    <interpolation>
      <entry><ind> 0.0 </ind><dep> 0.00 </dep></entry>
      <entry><ind> 0.2 </ind><dep> 0.08 </dep></entry>
      <entry><ind> 1.0 </ind><dep> 0.08 </dep></entry>
    </interpolation>
    <axis>
      <x> 0 </x>
      <y> 1 </y>
      <z> 0 </z>
    </axis>
  </animation>

