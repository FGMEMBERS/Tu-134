<PropertyList>
  
  <path>idr.ac</path>
  
  <animation>
    <object-name>fond</object-name>
    
    <object-name>face</object-name>
    <object-name>dig_hund</object-name>
    <object-name>dig_dec</object-name>
    <object-name>dig_ones</object-name>
    <object-name>dig_hm</object-name>
    <object-name>blanker</object-name>
    
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>noshadow</type>
    <object-name>vitre</object-name>
  </animation>

  <animation>
    <type>shader</type>
    <shader>chrome</shader>
    <texture>Aircraft/Generic/Effects/glass_shader.png</texture>
    <object-name>vitre</object-name>
  </animation>

  <!-- working for Rembrandt -->
  <effect>
    <inherits-from>Effects/model-transparent</inherits-from>
    <condition>
      <property>sim/rendering/rembrandt/enabled</property>
    </condition>
    <object-name>vitre</object-name>
  </effect>

  <animation>
    <type>material</type>
    <object-name>face</object-name>
    <object-name>dig_hund</object-name>
    <object-name>dig_dec</object-name>
    <object-name>dig_ones</object-name>
    <object-name>dig_hm</object-name>
    <object-name>blanker</object-name>
    <emission>
      <factor-prop>/systems/electrical/outputs/instrument-lights</factor-prop>
      <red>   0.50 </red>
      <green> 0.25 </green>
      <blue>  0.25 </blue>
    </emission>
  </animation>

  <!--Digit wheels-->
  <animation>
    <type>rotate</type>
    <object-name>dig_hm</object-name>
    <property>tu134/instrumentation/idr-1[0]/indicated-wheels_dec_m</property>
    <factor> 36 </factor>
    <center>
      <x-m> -0.02264 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.00000 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>dig_ones</object-name>
    <property>tu134/instrumentation/idr-1[0]/indicated-wheels_hund_m</property>
    <interpolation>
      <entry><ind> 0.00 </ind><dep>   0.0 </dep></entry>
      <entry><ind> 0.90 </ind><dep>   0.1 </dep></entry>
      <entry><ind> 1.00 </ind><dep>  36.0 </dep></entry>
      <entry><ind> 1.90 </ind><dep>  36.1 </dep></entry>
      <entry><ind> 2.00 </ind><dep>  72.0 </dep></entry>
      <entry><ind> 2.90 </ind><dep>  72.1 </dep></entry>
      <entry><ind> 3.00 </ind><dep> 108.0 </dep></entry>
      <entry><ind> 3.90 </ind><dep> 108.1 </dep></entry>
      <entry><ind> 4.00 </ind><dep> 144.0 </dep></entry>
      <entry><ind> 4.90 </ind><dep> 144.1 </dep></entry>
      <entry><ind> 5.00 </ind><dep> 180.0 </dep></entry>
      <entry><ind> 5.90 </ind><dep> 180.1 </dep></entry>
      <entry><ind> 6.00 </ind><dep> 216.0 </dep></entry>
      <entry><ind> 6.90 </ind><dep> 216.1 </dep></entry>
      <entry><ind> 7.00 </ind><dep> 252.0 </dep></entry>
      <entry><ind> 7.90 </ind><dep> 252.1 </dep></entry>
      <entry><ind> 8.00 </ind><dep> 288.0 </dep></entry>
      <entry><ind> 8.90 </ind><dep> 288.1 </dep></entry>
      <entry><ind> 9.00 </ind><dep> 324.0 </dep></entry>
      <entry><ind> 9.90 </ind><dep> 324.1 </dep></entry>
      <entry><ind> 9.99 </ind><dep> 359.9 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.02264 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.00000 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>dig_dec</object-name>
    <property>tu134/instrumentation/idr-1[0]/indicated-wheels_ths_m</property>
    <interpolation>
      <entry><ind> 0.000 </ind><dep>   0.0 </dep></entry>
      <entry><ind> 0.990 </ind><dep>   0.1 </dep></entry>
      <entry><ind> 1.000 </ind><dep>  36.0 </dep></entry>
      <entry><ind> 1.990 </ind><dep>  36.1 </dep></entry>
      <entry><ind> 2.000 </ind><dep>  72.0 </dep></entry>
      <entry><ind> 2.990 </ind><dep>  72.1 </dep></entry>
      <entry><ind> 3.000 </ind><dep> 108.0 </dep></entry>
      <entry><ind> 3.990 </ind><dep> 108.1 </dep></entry>
      <entry><ind> 4.000 </ind><dep> 144.0 </dep></entry>
      <entry><ind> 4.990 </ind><dep> 144.1 </dep></entry>
      <entry><ind> 5.000 </ind><dep> 180.0 </dep></entry>
      <entry><ind> 5.990 </ind><dep> 180.1 </dep></entry>
      <entry><ind> 6.000 </ind><dep> 216.0 </dep></entry>
      <entry><ind> 6.990 </ind><dep> 216.1 </dep></entry>
      <entry><ind> 7.000 </ind><dep> 252.0 </dep></entry>
      <entry><ind> 7.990 </ind><dep> 252.1 </dep></entry>
      <entry><ind> 8.000 </ind><dep> 288.0 </dep></entry>
      <entry><ind> 8.990 </ind><dep> 288.1 </dep></entry>
      <entry><ind> 9.000 </ind><dep> 324.0 </dep></entry>
      <entry><ind> 9.990 </ind><dep> 324.1 </dep></entry>
      <entry><ind> 9.999 </ind><dep> 359.9 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.02264 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.00000 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <animation>
    <type>rotate</type>
    <object-name>dig_hund</object-name>
    <property>tu134/instrumentation/idr-1[0]/indicated-wheels_decths_m</property>
    <interpolation>
      <entry><ind> 0.0000 </ind><dep>   0.0 </dep></entry>
      <entry><ind> 0.9990 </ind><dep>   0.1 </dep></entry>
      <entry><ind> 1.0000 </ind><dep>  36.0 </dep></entry>
      <entry><ind> 1.9990 </ind><dep>  36.1 </dep></entry>
      <entry><ind> 2.0000 </ind><dep>  72.0 </dep></entry>
      <entry><ind> 2.9990 </ind><dep>  72.1 </dep></entry>
      <entry><ind> 3.0000 </ind><dep> 108.0 </dep></entry>
      <entry><ind> 3.9990 </ind><dep> 108.1 </dep></entry>
      <entry><ind> 4.0000 </ind><dep> 144.0 </dep></entry>
      <entry><ind> 4.9990 </ind><dep> 144.1 </dep></entry>
      <entry><ind> 5.0000 </ind><dep> 180.0 </dep></entry>
      <entry><ind> 5.9990 </ind><dep> 180.1 </dep></entry>
      <entry><ind> 6.0000 </ind><dep> 216.0 </dep></entry>
      <entry><ind> 6.9990 </ind><dep> 216.1 </dep></entry>
      <entry><ind> 7.0000 </ind><dep> 252.0 </dep></entry>
      <entry><ind> 7.9990 </ind><dep> 252.1 </dep></entry>
      <entry><ind> 8.0000 </ind><dep> 288.0 </dep></entry>
      <entry><ind> 8.9990 </ind><dep> 288.1 </dep></entry>
      <entry><ind> 9.0000 </ind><dep> 324.0 </dep></entry>
      <entry><ind> 9.9990 </ind><dep> 324.1 </dep></entry>
      <entry><ind> 9.9999 </ind><dep> 359.9 </dep></entry>
    </interpolation>
    <center>
      <x-m> -0.02264 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.00000 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

  <!--Blanker-->
  <animation>
    <type>rotate</type>
    <object-name>blanker</object-name>
    <property>tu134/instrumentation/idr-1[0]/caged-flag</property>
    <offset-deg> -90 </offset-deg>
    <factor> 90 </factor>
    <center>
      <x-m> -0.02264 </x-m>
      <y-m>  0.00000 </y-m>
      <z-m>  0.00000 </z-m>
    </center>
    <axis>
      <x>  0 </x>
      <y> -1 </y>
      <z>  0 </z>
    </axis>
  </animation>

</PropertyList>
