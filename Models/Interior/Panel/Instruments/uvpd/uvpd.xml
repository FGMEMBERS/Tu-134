<?xml version="1.0"?>
<PropertyList>
 <path>uvpd.ac</path>
  
<!--Needles-->
<animation>
    <type>rotate</type>
    <object-name>needle_1</object-name>
    <property>fdm/jsbsim/propulsion/engine[0]/egt-filtered</property>
    <interpolation>
      <entry><ind>	0.0</ind>	<dep>	-134.0</dep></entry>
      <entry><ind>	600.0</ind>	<dep>	0.0</dep></entry>
      <entry><ind>	1200.0</ind>	<dep>	134.0</dep></entry>
    </interpolation>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
    <axis>
      <x>-1.0</x>
      <y>0.0</y>
      <z>0.0</z>
    </axis>
</animation>
<animation>
  <type>rotate</type>
  <object-name>needle_2</object-name>
  <property>fdm/jsbsim/propulsion/engine[0]/egt-filtered</property>
  <factor>3.6</factor>
  <offset-deg>180.0</offset-deg>
  <center>
    <x-m>0.0</x-m>
    <y-m>0.0</y-m>
    <z-m>0.0</z-m>
  </center>
  <axis>
    <x>-1.0</x>
    <y>0.0</y>
    <z>0.0</z>
  </axis>
</animation>


</PropertyList>
